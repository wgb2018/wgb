<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.microdev.mapper.EvaluteGradeMapper">
    <sql id="base_column">
        id as pid, create_time, deleted, modify_time, role_id, grade
    </sql>

    <insert id="saveInfo" parameterType="com.microdev.model.EvaluteGrade">
        insert into evalute_grade values (#{id}, #{createTime}, #{modifyTime}, #{deleted}, #{roleId}, #{grade})
    </insert>
</mapper>